<Window x:Class="InterfazNova.NumVentasMes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfazNova"
        mc:Ignorable="d"
        Title="NumVentasMes" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Label Content="Resumen Total de ventas" HorizontalAlignment="Center" Grid.Row="0" VerticalAlignment="Top" FontSize="24" FontFamily="Arial Rounded MT Bold" Margin="0,10,0,0"/>
        <StackPanel Margin="109,10,109,88" Grid.Row="1">
            <Label x:Name="numVentas" Content="*Num Ventas*" HorizontalAlignment="Center" FontSize="36"/>
            <ProgressBar Height="28" Width="524" LargeChange="0"/>

        </StackPanel>


    </Grid>
</Window>
